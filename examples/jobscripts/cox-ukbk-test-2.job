#!/bin/sh
#$ -cwd
#$ -N cox-ukbk-2
#$ -pe mpi 72
#$ -j y
date
module load mpi/openmpi-x86_64
export LAMBDA=$1
. /shared/julia_setup.sh
export OMP_NUM_THREADS=1
mpirun -np 72 julia --project=/shared/julia_pkgs/dev/DistStat/examples /shared/julia_pkgs/dev/DistStat/examples/cox-breslow-ukbk-test.jl --lambda=$LAMBDA --step=10 --iter=2100 --eval_obj
