#!/bin/sh
#$ -cwd
#$ -N mds
#$ -pe mpi 8
#$ -j y
date
module load mpi/openmpi-x86_64
export ROWS=$1
export COLS=$2
export R=$3
. /shared/julia_setup.sh
export OMP_NUM_THREADS=18
echo $ROWS
mpirun -np 8 julia --project=/shared/julia_pkgs/dev/DistStat/examples /shared/julia_pkgs/dev/DistStat/examples/mds.jl --rows=$ROWS --cols=$COLS --r=$R 
