#!/bin/sh
#$ -cwd
#$ -N cox-ukbk
#$ -pe mpi 720
#$ -j y
date
module load mpi/openmpi-x86_64
export LAMBDA=$1
. /shared/julia_setup.sh
export OMP_NUM_THREADS=1
mpirun -np 720 julia --project=/shared/julia_pkgs/dev/DistStat/examples /shared/julia_pkgs/dev/DistStat/examples/cox-breslow-ukbk-200k.jl --lambda=$LAMBDA --step=10 --iter=5000 --eval_obj --prefix 200k_dp_$LAMBDA 
